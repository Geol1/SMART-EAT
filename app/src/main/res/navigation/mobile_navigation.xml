<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.resto.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_navigation_home_to_search"
            app:destination="@id/search" />
    </fragment>

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.resto.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" >
        <action
            android:id="@+id/action_navigation_dashboard_to_collectionByFozzi2"
            app:destination="@id/collectionByFozzi" />
    </fragment>
    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.resto.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/collectionByFozzi"
        android:name="com.example.resto.ui.CollectionByFozzi.CollectionByFozzi"
        android:label="popular_this_week_fragment"
        tools:layout="@layout/popular_this_week_fragment" >
        <action
            android:id="@+id/action_collectionByFozzi_to_asiaRestaurantFragment"
            app:destination="@id/asiaRestaurantFragment" />
    </fragment>
    <fragment
        android:id="@+id/asiaRestaurantFragment"
        android:name="com.example.resto.ui.restaurant.AsiaRestaurantFragment"
        android:label="asia_restaurant_fragment"
        tools:layout="@layout/asia_restaurant_fragment" />
    <fragment
        android:id="@+id/search"
        android:name="com.example.resto.ui.search.Search"
        android:label="search_fragment"
        tools:layout="@layout/search_fragment" />

</navigation>