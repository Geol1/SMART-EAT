<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginBottom="@dimen/dimens_60sp"
    tools:context=".ui.profile.ProfileFragment">
    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:padding="@dimen/dimens_20dp"
                android:background="#E5E5EA"
                app:layout_constraintBottom_toTopOf="@id/mainBody"
                app:layout_constraintEnd_toEndOf="parent">
                <androidx.cardview.widget.CardView
                    android:layout_width="@dimen/dimens_100dp"
                    android:layout_height="@dimen/dimens_100dp"
                    app:cardCornerRadius="@dimen/dimens_50dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:id="@+id/imgProfile"
                    >
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@mipmap/oldrich_round"
                        />
                </androidx.cardview.widget.CardView>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toEndOf="@id/imgProfile"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent">
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:id="@+id/idLinearname"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Cameron Cook"
                            android:textColor="@color/black"
                            android:textStyle="bold"
                            android:fontFamily="@font/avenir_ltstd_black"
                            android:textSize="@dimen/dimens_20dp"
                            android:layout_marginEnd="@dimen/dimens_20dp"
                            android:id="@+id/iduserName"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/arrow0"
                            app:layout_constraintStart_toStartOf="parent"/>
                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/ic_baseline_chevron_right_24"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            android:id="@+id/arrow0" />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <ImageButton
                        android:id="@+id/idVIPbutton"
                        android:layout_width="75dp"
                        android:layout_height="@dimen/dimens_20sp"
                        android:src="@mipmap/img_vip_foreground"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginTop="@dimen/dimens_10dp"
                        app:layout_constraintTop_toBottomOf="@id/idLinearname"
                        app:layout_constraintBottom_toBottomOf="parent" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                app:layout_constraintHeight_default="percent"
                app:layout_constraintHeight_percent="0.75"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:background="@color/white"
                android:id="@+id/mainBody">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:paddingHorizontal="@dimen/dimens_20dp"
                    android:orientation="vertical"
                    >
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dimens_20dp"
                        >
                        <ImageView
                            android:layout_width="@dimen/dimens_40dp"
                            android:layout_height="@dimen/dimens_40dp"
                            android:src="@mipmap/img_notification_foreground"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            android:id="@+id/img1"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/notifications"
                            android:fontFamily="@font/avenir_ltstd_book"
                            android:textSize="@dimen/dimens_17sp"
                            app:layout_constraintStart_toEndOf="@id/img1"
                            android:layout_marginStart="@dimen/dimens_10dp"
                            android:textColor="@color/black"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            />
                        <ImageView
                            android:layout_width="@dimen/dimens_20dp"
                            android:layout_height="@dimen/dimens_20dp"
                            android:src="@drawable/ic_baseline_chevron_right_24"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:id="@+id/arrow1"
                            />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        >
                        <ImageView
                            android:layout_width="@dimen/dimens_40dp"
                            android:layout_height="@dimen/dimens_40dp"
                            android:src="@mipmap/img_payment_foreground"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            android:id="@+id/img2"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/payment_method"
                            app:layout_constraintStart_toEndOf="@id/img2"
                            android:layout_marginStart="@dimen/dimens_10dp"
                            android:fontFamily="@font/avenir_ltstd_book"
                            android:textSize="@dimen/dimens_17sp"
                            android:textColor="@color/black"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            />
                        <ImageView
                            android:layout_width="@dimen/dimens_20dp"
                            android:layout_height="@dimen/dimens_20dp"
                            android:src="@drawable/ic_baseline_chevron_right_24"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:id="@+id/arrow2"
                            />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        >
                        <ImageView
                            android:layout_width="@dimen/dimens_40dp"
                            android:layout_height="@dimen/dimens_40dp"
                            android:src="@mipmap/img_rewards_foreground"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            android:id="@+id/img3"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/reward_credits"
                            app:layout_constraintStart_toEndOf="@id/img3"
                            android:layout_marginStart="@dimen/dimens_10dp"
                            android:fontFamily="@font/avenir_ltstd_book"
                            android:textSize="@dimen/dimens_17sp"
                            android:textColor="@color/black"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            />
                        <ImageView
                            android:layout_width="@dimen/dimens_20dp"
                            android:layout_height="@dimen/dimens_20dp"
                            android:src="@drawable/ic_baseline_chevron_right_24"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:id="@+id/arrow3"
                            />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        >
                        <ImageView
                            android:layout_width="@dimen/dimens_40dp"
                            android:layout_height="@dimen/dimens_40dp"
                            android:src="@mipmap/img_promo_code_foreground"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            android:id="@+id/img4"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/my_promo_code"
                            app:layout_constraintStart_toEndOf="@id/img4"
                            android:layout_marginStart="@dimen/dimens_10dp"
                            android:fontFamily="@font/avenir_ltstd_book"
                            android:textSize="@dimen/dimens_17sp"
                            android:textColor="@color/black"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            />
                        <ImageView
                            android:layout_width="@dimen/dimens_20dp"
                            android:layout_height="@dimen/dimens_20dp"
                            android:src="@drawable/ic_baseline_chevron_right_24"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:id="@+id/arrow4"
                            />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dimens_50dp"
                        >
                        <ImageView
                            android:layout_width="@dimen/dimens_40dp"
                            android:layout_height="@dimen/dimens_40dp"
                            android:src="@mipmap/img_settings_foreground"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            android:id="@+id/img5"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/settings"
                            android:fontFamily="@font/avenir_ltstd_book"
                            android:textSize="@dimen/dimens_17sp"
                            app:layout_constraintStart_toEndOf="@id/img5"
                            android:layout_marginStart="@dimen/dimens_10dp"
                            android:textColor="@color/black"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            />
                        <ImageView
                            android:layout_width="@dimen/dimens_20dp"
                            android:layout_height="@dimen/dimens_20dp"
                            android:src="@drawable/ic_baseline_chevron_right_24"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:id="@+id/arrow5"
                            />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        >
                        <ImageView
                            android:layout_width="@dimen/dimens_40dp"
                            android:layout_height="@dimen/dimens_40dp"
                            android:src="@mipmap/img_invite_friends_foreground"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            android:id="@+id/img6"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/invite_friends"
                            app:layout_constraintStart_toEndOf="@id/img6"
                            android:layout_marginStart="@dimen/dimens_10dp"
                            android:fontFamily="@font/avenir_ltstd_book"
                            android:textSize="@dimen/dimens_17sp"
                            android:textColor="@color/black"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            />
                        <ImageView
                            android:layout_width="@dimen/dimens_20dp"
                            android:layout_height="@dimen/dimens_20dp"
                            android:src="@drawable/ic_baseline_chevron_right_24"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:id="@+id/arrow6"
                            />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        >
                        <ImageView
                            android:layout_width="@dimen/dimens_40dp"
                            android:layout_height="@dimen/dimens_40dp"
                            android:src="@mipmap/img_help_center_foreground"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            android:id="@+id/img7"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/help_center"
                            app:layout_constraintStart_toEndOf="@id/img7"
                            android:layout_marginStart="@dimen/dimens_10dp"
                            android:textColor="@color/black"
                            android:fontFamily="@font/avenir_ltstd_book"
                            android:textSize="@dimen/dimens_17sp"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            />
                        <ImageView
                            android:layout_width="@dimen/dimens_20dp"
                            android:layout_height="@dimen/dimens_20dp"
                            android:src="@drawable/ic_baseline_chevron_right_24"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:id="@+id/arrow7"
                            />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        >
                        <ImageView
                            android:layout_width="@dimen/dimens_40dp"
                            android:layout_height="@dimen/dimens_40dp"
                            android:src="@mipmap/img_about_us_foreground"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            android:id="@+id/img8"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/about_us"
                            app:layout_constraintStart_toEndOf="@id/img8"
                            android:layout_marginStart="@dimen/dimens_10dp"
                            android:textColor="@color/black"
                            android:fontFamily="@font/avenir_ltstd_book"
                            android:textSize="@dimen/dimens_17sp"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            />
                        <ImageView
                            android:layout_width="@dimen/dimens_20dp"
                            android:layout_height="@dimen/dimens_20dp"
                            android:src="@drawable/ic_baseline_chevron_right_24"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:id="@+id/arrow8"
                            />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                </LinearLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>